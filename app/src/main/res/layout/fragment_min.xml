<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/color_efefef"
    tools:context=".ui.fragment.TienMinFragment">

    <include
        android:id="@+id/head_id"
        layout="@layout/return_layout1" />

    <RelativeLayout
        android:id="@+id/rl1_id"
        android:layout_width="match_parent"
        android:layout_height="@dimen/space_90"
        android:layout_below="@id/head_id"
        android:layout_marginStart="@dimen/space_40"
        android:layout_marginTop="@dimen/space_30"
        android:layout_marginEnd="@dimen/space_40"
        android:background="@drawable/shape_b3d5ff_10dp"
        android:gravity="center_vertical">

        <ImageView
            android:id="@id/iv1_id"
            android:src="@mipmap/logo"
            android:layout_width="@dimen/space_60"
            android:layout_height="@dimen/space_60"
            android:layout_marginStart="@dimen/space_20" />

        <TextView
            android:id="@+id/tv1_id"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignTop="@id/iv1_id"
            android:layout_marginStart="@dimen/space_15"
            android:layout_marginTop="@dimen/space_5"
            android:layout_toEndOf="@id/iv1_id"
            android:textColor="@color/black"
            android:textSize="@dimen/sp_12" />

        <TextView
            android:id="@+id/tv2_id"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignBottom="@id/iv1_id"
            android:layout_marginStart="@dimen/space_15"
            android:layout_marginBottom="@dimen/space_5"
            android:layout_toEndOf="@id/iv1_id"
            android:textColor="@color/black"
            android:textSize="@dimen/sp_12" />
    </RelativeLayout>

    <TextView
        android:id="@+id/tv3_id"
        android:layout_width="match_parent"
        android:layout_height="@dimen/space_30"
        android:layout_below="@id/rl1_id"
        android:layout_marginStart="@dimen/space_40"
        android:layout_marginTop="@dimen/space_40"
        android:layout_marginEnd="@dimen/space_40"
        android:drawableStart="@mipmap/min1"
        android:drawableEnd="@mipmap/min6"
        android:drawablePadding="@dimen/space_10"
        android:gravity="center_vertical"
        android:text="@string/min1"
        android:textColor="@color/black"
        android:textSize="@dimen/sp_12" />

    <TextView
        android:id="@+id/tv4_id"
        android:layout_width="match_parent"
        android:layout_height="@dimen/space_30"
        android:layout_below="@id/tv3_id"
        android:layout_marginStart="@dimen/space_40"
        android:layout_marginTop="@dimen/space_15"
        android:layout_marginEnd="@dimen/space_40"
        android:drawableStart="@mipmap/min2"
        android:drawableEnd="@mipmap/min6"
        android:drawablePadding="@dimen/space_10"
        android:gravity="center_vertical"
        android:text="@string/min2"
        android:textColor="@color/black"
        android:textSize="@dimen/sp_12" />

    <TextView
        android:id="@+id/tv5_id"
        android:layout_width="match_parent"
        android:layout_height="@dimen/space_30"
        android:layout_below="@id/tv4_id"
        android:layout_marginStart="@dimen/space_40"
        android:layout_marginTop="@dimen/space_15"
        android:layout_marginEnd="@dimen/space_40"
        android:drawableStart="@mipmap/min3"
        android:drawableEnd="@mipmap/min6"
        android:drawablePadding="@dimen/space_10"
        android:gravity="center_vertical"
        android:text="@string/min3"
        android:textColor="@color/black"
        android:textSize="@dimen/sp_12" />

    <TextView
        android:id="@+id/tv6_id"
        android:layout_width="match_parent"
        android:layout_height="@dimen/space_30"
        android:layout_below="@id/tv5_id"
        android:layout_marginStart="@dimen/space_40"
        android:layout_marginTop="@dimen/space_15"
        android:layout_marginEnd="@dimen/space_40"
        android:drawableStart="@mipmap/min4"
        android:drawableEnd="@mipmap/min6"
        android:drawablePadding="@dimen/space_10"
        android:gravity="center_vertical"
        android:text="@string/min4"
        android:textColor="@color/black"
        android:textSize="@dimen/sp_12" />

    <TextView
        android:id="@+id/tv7_id"
        android:layout_width="match_parent"
        android:layout_height="@dimen/space_30"
        android:layout_below="@id/tv6_id"
        android:layout_marginStart="@dimen/space_40"
        android:layout_marginTop="@dimen/space_15"
        android:layout_marginEnd="@dimen/space_40"
        android:drawableStart="@mipmap/min5"
        android:drawableEnd="@mipmap/min6"
        android:drawablePadding="@dimen/space_10"
        android:gravity="center_vertical"
        android:text="@string/min5"
        android:textColor="@color/black"
        android:textSize="@dimen/sp_12" />
</RelativeLayout>